{
  "expo": {
    "name": "B-Live",
    "slug": "b-live",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons.png",
    "scheme": "b-live",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blives.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.blives.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons.png",
        "backgroundColor": "#13f28c"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icons.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#222222",
          "image": "./assets/images/icons.png",
          "dark": {
            "image": "./assets/images/icons.png",
            "backgroundColor": "#13f28c"
          },
          "resizeMode": "contain",
          "imageWidth": 200
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "a278a74e-efbd-4986-bd91-969c60ed1754"
      }
    }
  }
}
