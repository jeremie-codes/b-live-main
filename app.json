{
  "expo": {
    "name": "B-Live",
    "slug": "b-live",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons.png",
    "scheme": "b-live",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon-app.png"
    },
    "plugins": [
      "expo-router", "expo-font", "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#222222",
          "image": "./assets/images/splashs.png",
          "dark": {
            "image": "./assets/images/splashs.png",
            "backgroundColor": "#13f28c"
          },
          "resizeMode": "contain",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
